{
  "name": "mi-web-builder",
  "version": "1.0.0",
  "description": "Sistema de Web Builder moderno con TypeScript similar a AEM o WordPress",
  "main": "dist/server/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "list": "pnpm -r list",
    "lint": "eslint \"packages/*/src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint --fix \"packages/*/src/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"packages/*/src/**/*.{ts,tsx,css,scss,json,md}\"",
    "format:check": "prettier --check \"packages/*/src/**/*.{ts,tsx,css,scss,json,md}\"",
    "type-check": "tsc --build",
    "dev": "pnpm -r --parallel run dev",
    "build": "pnpm -r run build",
    "clean": "pnpm -r run clean && rm -rf node_modules"
  },
  "keywords": [
    "web-builder",
    "cms",
    "typescript",
    "react",
    "node"
  ],
  "author": "Veriel Dev",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "typescript-eslint": "^8.24.1",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.4.2",
    "typescript": "^5.8.2"
  }
}
